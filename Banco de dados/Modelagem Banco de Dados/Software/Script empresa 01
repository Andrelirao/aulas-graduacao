/* Lógico_1: */

CREATE TABLE FUNCIONARIO (
    Nome INTEGER,
    CPF CHAR,
    Logradouro VARCHAR,
    Bairro VARCHAR,
    Cidade VARCHAR,
    CEP VARCHAR,
    UF CHAR,
    Cargo VARCHAR,
    ID INTEGER PRIMARY KEY,
    FK_CONJUGE_ID INTEGER
);

CREATE TABLE DEPENDENTE (
    IDFunc INTEGER,
    Nome VARCHAR,
    CPF CHAR
);

CREATE TABLE CONJUGE (
    ID INTEGER PRIMARY KEY,
    Nome VARCHAR,
    IDFunc INTEGER,
    CPF CHAR
);
 
ALTER TABLE FUNCIONARIO ADD CONSTRAINT FK_FUNCIONARIO_2
    FOREIGN KEY (FK_CONJUGE_ID)
    REFERENCES CONJUGE (ID)
    ON DELETE CASCADE;
 
ALTER TABLE DEPENDENTE ADD CONSTRAINT FK_DEPENDENTE_1
    FOREIGN KEY (IDFunc???)
    REFERENCES ??? (???);
 
ALTER TABLE CONJUGE ADD CONSTRAINT FK_CONJUGE_2
    FOREIGN KEY (IDFunc???)
    REFERENCES ??? (???);